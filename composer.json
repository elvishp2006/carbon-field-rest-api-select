{
  "name": "elvishp2006/carbon-field-rest-api-select",
  "description": "Carbon Fields extension, that adds a rest_api_select field type.",
  "version": "0.2.1",
  "keywords": [
    "wordpress",
    "carbon-field",
    "carbon-field-rest-api-select"
  ],
  "type": "library",
  "license": "GPL-2.0+",
  "authors": [],
  "require": {
    "htmlburger/carbon-fields": "^2.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "squizlabs/php_codesniffer": "^3.3",
    "phpcompatibility/phpcompatibility-wp": "^1.0",
    "wp-coding-standards/wpcs": "^1.0"
  },
  "autoload": {
    "files": [
      "field.php"
    ],
    "psr-4": {
      "Carbon_Field_Rest_Api_Select\\": "core/"
    }
  },
  "scripts": {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ],
    "phpcs": "./vendor/bin/phpcs"
  }
}
